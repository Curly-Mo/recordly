<%= simple_form_for(@favorite) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.association :artist %>
    <%= f.association :album %>
    <%= f.association :song %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
